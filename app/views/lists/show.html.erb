<h1><%= @list.name %></h1>
<%= cl_image_tag @list.photo.key, height: 300, width: 400, crop: :fill %>
<% @list.bookmarks.each do |bookmark| %>
<% movie = bookmark.movie %>
<%= movie.title %> <%= link_to 'delete', bookmark_path(bookmark), method: :delete %>
<% end %>
<%= link_to 'Add Movie', new_list_bookmark_path(@list) %>
<%= link_to 'Home', lists_path  %>
